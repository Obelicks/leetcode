   "source": [
    "def is_palindome(x: int) -> bool:\n",
    "    val = [num for num in str(x)]\n",
    "    for i in range(len(val)):\n",
    "        if i >= len(val)/2:\n",
    "            return(True)\n",
    "        if val[i] != val[-1*(i+1)]:\n",
    "            return(False)\n",
    "    return(True)"
   ]
